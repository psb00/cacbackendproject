{{>layouts/header}}



<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
        <th scope="col">File Size</th>
    </tr>
    </thead>
    <tbody>
    {{#article}}
    <tr>
        <th>{{id}}</th>
        <td>{{title}}</td>
        <td>{{content}}</td>
        <td>{{filesize}}byte</td>

    </tr>

    {{/article}}
    </tbody>
</table>
<a href="/article/{{article.id}}/edit" class="btn btn-primary">수정하기</a>

<a href="/article" class="btn btn-primary" >게시판으로</a>



{{>layouts/footer}}